{
  "name": "dynamsoft-node-barcode",
  "version": "8.8.7",
  "description": "Dynamsoft Barcode Reader Node is a recognition SDK which enables you to embed barcode reading functionality in your applications. With a few lines of JavaScript code, you can develop a robust application to scan a linear barcode, QR Code, DaraMatrix, PDF417, and Aztec Code.",
  "files": [
    "/dist/dbr.pure.js",
    "/dist/dbr-*.worker.js",
    "/dist/dbr-*.wasm.js",
    "/dist/dbr-*.wasm",
    "/dist/dls.license.dialog.html",
    "/doc",
    "/samples.url"
  ],
  "homepage": "https://www.dynamsoft.com/barcode-reader/sdk-javascript",
  "main": "dist/dbr.pure.js",
  "author": {
    "name": "Dynamsoft",
    "url": "https://www.dynamsoft.com"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "maintainers": [
    {
      "name": "Dynamsoft",
      "email": "support@dynamsoft.com"
    }
  ],
  "keywords": [
    "Node.js barcode",
    "JavaScript barcode",
    "barcode",
    "PDF417",
    "QRCode",
    "Aztec Code",
    "WebAssembly",
    "SDK",
    "Linear barcode",
    "1D barcode"
  ],
  "scripts": {
    "build:doc": "typedoc",
    "build-dev": "rollup -c --environment BUILD:development",
    "build": "rollup -c --environment BUILD:production",
    "test": "node ./samples/node/helloworld.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/node": "^16.10.1",
    "fs-extra": "^8.1.0",
    "localforage": "^1.7.3",
    "rollup": "^2.57.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.5",
    "typedoc-plugin-missing-exports": "^0.22.3",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "node-fetch": "^2.6.5",
    "node-localstorage": "^2.1.6"
  },
  "dependencies": {}
